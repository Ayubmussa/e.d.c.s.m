      { "name": "Get Geofencing Status", "request": { "method": "GET", "url": { "raw": "{{base_url}}/geofencing/status", "host": ["{{base_url}}"], "path": ["geofencing", "status"] } } },
      { "name": "Get Zone History", "request": { "method": "GET", "url": { "raw": "{{base_url}}/geofencing/zone-history/{{zone_id}}", "host": ["{{base_url}}"], "path": ["geofencing", "zone-history", "{{zone_id}}"] } } }
    ] },
    
